{
  "name": "@natlibfi/identifier-services-ui",
  "description": "UI microservice of Identifier Services",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [],
  "homepage": "https://github.com/natlibfi/identifier-services-ui",
  "bugs": {
    "url": "https://github.com/natlibfi/identifier-services-ui/issues"
  },
  "browser": {
    "child_process": false
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:natlibfi/identifier-services-ui.git"
  },
  "license": "AGPL-3.0+",
  "version": "1.0.0",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node dist/index.js",
    "lint": "eslint --ext .jsx,.js src",
    "lint:dev": "eslint --fix --ext .jsx,.js src",
    "watch:dev": "DEBUG=1 webpack-dev-server --open --config=./webpack/webpack.config.dev.js --hot --progress --history-api-fallback",
    "build": "npm run build:server && npm run build:frontend",
    "build:frontend": "export NODE_ENV=production & NODE_ENV=production & webpack --config ./webpack/webpack.config.prod.js --progress",
    "build:server": "cd src/server && BABEL_ENV=production babel . --out-dir ../../dist --source-maps"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/styles": "^4.11.4",
    "@natlibfi/identifier-services-commons": "git+https://github.com/natlibfi/identifier-services-commons.git#feature/apiError",
    "express": "^4.17.2",
    "http-status": "^1.4.2",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "quill": "^1.3.7",
    "react": "^17.0.2",
    "react-cookie": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-intl": "^3.4.0",
    "react-quilljs": "^1.2.12",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "react-to-print": "^2.12.3",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.4",
    "redux-thunk": "^2.3.0",
    "string-template-js": "^1.0.3",
    "validator": "^12.0.0",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.14.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import"
    ],
    "extends": [
      "xo",
      "xo-react",
      "plugin:import/errors"
    ],
    "rules": {
      "prefer-arrow-callback": [
        "error"
      ],
      "default-param-last": [
        "off"
      ],
      "react/jsx-no-bind": [
        "off"
      ]
    },
    "env": {
      "browser": true
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  }
}
